static Class Math()
    this.PI = 3.14

    this.floor = function(x)
    {
        n = x div 1
        if x >= 0 then
            return n
        end if
        if x == n then
            return n
        end if
        return n - 1
    }

    this.ceil = function(x)
    {
        return -(this.floor(-x))
    }

    this.round = function(x)
    {
        f = this.floor(x)
        c = this.ceil(x)
        if x - f < 0.5 then
            return f
        end if
        return c
    }

    this.max = function(a, b)
    {
        if a >= b then
            return a
        end if
        return b
    }

    this.min = function(a, b)
    {
        if a <= b then
            return a
        end if
        return b
    }
end Class